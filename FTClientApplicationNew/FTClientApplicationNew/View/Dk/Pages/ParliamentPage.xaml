<Page x:Class="FTClientApplication.View.Dk.Pages.ParliamentPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FTClientApplication.View.Dk.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Parliament">

    <DockPanel>
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
            <Button Height="20" Width="150" Click="Button_Click">Create Parliament</Button>
        </StackPanel>
        <DataGrid>
            <DataGrid.Columns>
                <DataGridTextColumn Header="Fornavn" Width="*"></DataGridTextColumn>
                <DataGridTextColumn Header="Efternavn" Width="*"></DataGridTextColumn>
                <DataGridTextColumn Header="Telefon" Width="*"></DataGridTextColumn>
                <DataGridTextColumn Header="Email" Width="*"></DataGridTextColumn>
                <DataGridTextColumn Header="Parti" Width="*"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>
    </DockPanel>
</Page>
